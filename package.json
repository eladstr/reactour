{
  "name": "reactour",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18",
  "dependencies": {
    "@changesets/cli": "^2.27.11",
    "reactour-config": "workspace:*",
    "reactour-mask": "workspace:*",
    "reactour-popover": "workspace:*",
    "reactour-tour": "workspace:*",
    "reactour-tsconfig": "workspace:*",
    "reactour-utils": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eladstr/reactour.git"
  }
}
